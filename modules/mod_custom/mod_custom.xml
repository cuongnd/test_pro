<?xml version="1.0" encoding="UTF-8"?>
<extension type="element" version="3.1" client="site">
	<name>tabs</name>
	<author>Joomla! Project</author>
	<creationDate>July 2004</creationDate>
	<copyright>Copyright (C) 2005 - 2014 Open Source Matters. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<authorEmail>admin@joomla.org</authorEmail>
	<authorUrl>www.joomla.org</authorUrl>
	<version>3.0.0</version>
	<description>MOD_CUSTOM_XML_DESCRIPTION</description>
	<customContent />
	<files>
		<filename module="mod_custom">mod_custom.php</filename>
		<filename>index.html</filename>
		<folder>tmpl</folder>
	</files>
	<languages>
		<language tag="en-GB">en-GB.mod_custom.ini</language>
		<language tag="en-GB">en-GB.mod_custom.sys.ini</language>
	</languages>
	<help key="JHELP_EXTENSIONS_MODULE_MANAGER_CUSTOM_HTML" />
	<config>
		<fields name="params">
			<fields name="data">
				<fieldset name="data" description="Data config" label="Data config">
					<field name="bindingSource" readonly="readonly" type="bindingSource" description="bindingSource" label="bindingSource" />
				</fieldset>
			</fields>
		</fields>
		<fields name="params">
			<fieldset name="element" description="Element config" label="Element config">
				<field name="prepare_content" disablefrontend="1" type="radio" class="btn-group btn-group-yesno" label="MOD_CUSTOM_FIELD_PREPARE_CONTENT_LABEL" description="MOD_CUSTOM_FIELD_PREPARE_CONTENT_DESC" default="0">
					<option value="1">JYES</option>
					<option value="0">JNO</option>
				</field>
				<field name="backgroundimage" type="media" label="MOD_CUSTOM_FIELD_BACKGROUNDIMAGE_LABEL" description="MOD_BACKGROUNDIMAGE_FIELD_LOGO_DESC" />
				<field name="includefile" type="filelist" directory="modules/mod_custom/includefiles" label="MOD_CUSTOM_FIELD_INCLUDE_FILE_LABEL" description="MOD_INCLUDE_FILE_DESC" />
				<field name="layout" disablefrontend="1" type="modulelayout" label="JFIELD_ALT_LAYOUT_LABEL" description="JFIELD_ALT_MODULE_LAYOUT_DESC" />
				<field name="moduleclass_sfx" type="textarea" rows="3" label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL" description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
				<field name="cache" disablefrontend="1" type="list" default="1" label="COM_MODULES_FIELD_CACHING_LABEL" description="COM_MODULES_FIELD_CACHING_DESC">
					<option value="1">JGLOBAL_USE_GLOBAL</option>
					<option value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>
				</field>
				<field name="cache_time" disablefrontend="1" type="text" default="900" label="COM_MODULES_FIELD_CACHE_TIME_LABEL" description="COM_MODULES_FIELD_CACHE_TIME_DESC" />
				<field name="cachemode" disablefrontend="1" type="hidden" default="static">
					<option value="static" />
				</field>
			</fieldset>
		</fields>
	</config>
</extension>
