<?xml version="1.0" encoding="utf-8"?>
<form>
    <fieldset>
        <field name="id" type="text" class="readonly" label="JGLOBAL_FIELD_ID_LABEL" description="JGLOBAL_FIELD_ID_DESC" size="10" default="0" readonly="true" />
        <field name="title" type="text" label="JGLOBAL_TITLE" class="inputbox" size="100" required="true" />
        <field name="ordering" type="Ordering" class="inputbox"
			label="JFIELD_ORDERING_LABEL"
			description="JFIELD_ORDERING_DESC" />
        <field
			name="state"
			type="list"
			label="JSTATUS"
			description="COM_BOOKPRO_FIELD_STATE_DESC"
			class="inputbox span12 small"
			size="1"
			default="1">
			<option
				value="1">JPUBLISHED</option>
			<option
				value="0">JUNPUBLISHED</option>
		</field>
		<field name="access" type="accesslevel" label="JFIELD_ACCESS_LABEL"
			description="JFIELD_ACCESS_DESC" class="inputbox span12 small" size="1" />

		<field
			name="language"
			type="contentlanguage"
			label="JFIELD_LANGUAGE_LABEL"
			description="COM_WEBLINKS_FIELD_LANGUAGE_DESC"
			class="inputbox span12 small">
			<option value="*">JALL</option>
		</field>
		   
    </fieldset>
    
</form>